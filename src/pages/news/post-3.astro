import Layout from "../../layouts/post.astro";

export const frontmatter = {
  title: "令和7年五所川原立佞武多ポスターデザインコンペ出展者説明会について",
  pubDate: "2024-12-23",
  description:
    "令和7年五所川原立佞武多ポスターデザインコンペ出展者説明会について",
